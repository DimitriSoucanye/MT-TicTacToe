{
  "name": "mttictactoe",
  "version": "0.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "prettier --write \"*.{ts,tsx,js}\" \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:ci": "prettier --list-different \"*.{ts,tsx,js}\" \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "lint": "eslint \"*.{ts,tsx}\" \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "typings": "tsc --noEmit",
    "build:android": "eas build --profile mt-tictactoe-local --platform android --local",
    "build:ios": "eas build --profile mt-tictactoe-local --platform ios --local"
  },
  "dependencies": {
    "expo": "~51.0.28",
    "expo-application": "^5.9.1",
    "expo-dev-client": "~4.0.26",
    "expo-linking": "^6.3.1",
    "expo-router": "^3.5.23",
    "expo-status-bar": "~1.12.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "^18.3.8",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4",
    "prettier": "^3.3.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.6.2"
  },
  "volta": {
    "node": "21.1.0",
    "yarn": "1.22.22"
  },
  "private": true
}
